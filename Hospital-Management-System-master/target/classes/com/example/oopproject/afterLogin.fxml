            <Image url="@../../../img/pngtree-cartoon-case-for-diagnosis-and-treatment-image_1252592.jpg" />
         </image>
      </ImageView>
      <ImageView fitHeight="167.0" fitWidth="285.0" layoutX="-2.0" layoutY="147.0">
