            <Image url="@../../../img/istockphoto-1137949634-612x612.jpg" />
         </image>
      </ImageView>
      <Button fx:id="doctor" layoutX="387.0" layoutY="48.0" mnemonicParsing="false" onAction="#doctorInfo" text="Doctor">
